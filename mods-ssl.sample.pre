## This is your PEM-encoded server certificate
##
vserver!default!ssl_certificate_file = %sysconfdir%/cherokee/ssl/cherokee.pem

## This is the private key of your certificate, PEM-encoded.
##
vserver!default!ssl_certificate_key_file = %sysconfdir%/cherokee/ssl/cherokee.pem

## This is a file containing all certification authorities that you trust.
## Tip: you can just use the file cert.pem
##
vserver!default!ssl_ca_list_file = %sysconfdir%/cherokee/ssl/cherokee.pem
